    "# CÓDIGO REUTILIZABLE\n",
    "\n",
    "def cargar_csv(nombre_archivo):\n",
    "    ruta = os.path.join(\"./data\", nombre_archivo)\n",
    "    return pd.read_excel(ruta)"
